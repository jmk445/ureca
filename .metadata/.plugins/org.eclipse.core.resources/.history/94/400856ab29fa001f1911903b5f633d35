package app.phone.dto;

public class Phone {
	private int phoneId;
	private int phoneName;
	
	

	@Override
	public String toString() {
		return "Phone [phoneId=" + phoneId + ", phoneName=" + phoneName + "]";
	}

	public Phone(int phoneId, int phoneName) {
		this.phoneId = phoneId;
		this.phoneName = phoneName;
	}

	public int getPhoneId() {
		return phoneId;
	}

	public void setPhoneId(int phoneId) {
		this.phoneId = phoneId;
	}

	public int getPhoneName() {
		return phoneName;
	}

	public void setPhoneName(int phoneName) {
		this.phoneName = phoneName;
	}
	
	
	
}
